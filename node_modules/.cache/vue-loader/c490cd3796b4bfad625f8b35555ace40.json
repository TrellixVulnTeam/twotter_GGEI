","file":"/Users/justinjacobsumaya/Documents/laravel-vue-projects/twotter/src/components/TwootItem.vue","sourceRoot":"","sourcesContent":["<template>\n    <div class=\"twoot-item\" @click=\"favouriteTwoot(twoot.id)\">\n        <div class=\"user-profile__twoot\">\n            <div class=\"twoot-item__user\">\n                @{{ username }}\n            </div>\n            <div class=\"twoot-item__content\">\n                {{ twoot.content }}\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n\n\nexport default {\n    name: 'TwootItem',\n    props: {\n        username: {\n            type: String,\n            required: true\n        },\n        twoot: {\n            type: Object,\n            required: true\n        }\n    },\n    methods: {\n        favouriteTwoot(id) {\n            this.$emit('favourite', id);\n        }\n    }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n.twoot-item {\n    padding: 20px;\n    background-color: white;\n    border-radius: 5px;\n    border: 1px solid #DFE3E8;\n    box-sizing: border-box;\n    cursor: pointer;\n    transition: all 0.25s ease;\n\n    &:hover {\n        transform: scale(1.1, 1.1);\n    }\n\n    .twoot-item__user {\n        font-weight: bold;\n    }\n}\n</style>\n"]}]}